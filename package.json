{
  "name": "@wgr-sa/wi-kit",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "bin": {
    "wi-kit-init": "npm run install",
    "wi-kit-update": "npm run update"
  },
  "scripts": {
    "install": "copyfiles -u 1 \"./src/**/*.scss\" \"./assets\"",
    "update": "copyfiles -u 1 \"./src/scss/elements/*.scss\" \"./assets\""
  },
  "dependencies": {
    "copyfiles": "^2.4.1",
    "sass": "^1.54.9"
  }
}
